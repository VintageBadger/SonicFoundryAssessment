@model LocationViewModel

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-offset-4 col-md-offset-3 col-lg-offset-2">
            @if (Model.Weather != null) {
                <h2>Location Title</h2>
                <h3>Temp &deg;</h3>
                <h4>High: highTemp &deg; Low: lowTemp &deg;</h4>
            }
            <br />
            <br/>
            <button class="btn btn-danger">DELETE</button>
        </div>
    </div>
</div>
